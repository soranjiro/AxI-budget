# サンプル terraform.tfvars
# このファイルをコピーして terraform.tfvars を作成してください
# terraform.tfvars は .gitignore に含まれているため、個人の設定情報は安全です

# AWS Profile (required)
aws_profile = "axi-budget-stg"

# Project Configuration
project_name = "axi-budget"
environment  = "stg"
aws_region   = "ap-northeast-1"

# Cognito Configuration
cognito_callback_urls = [
  "https://stg-your-domain.com",
  "http://localhost:3000",
  "http://localhost:3001"
]

cognito_logout_urls = [
  "https://stg-your-domain.com",
  "http://localhost:3000",
  "http://localhost:3001"
]
